# Gemfile
source "https://rubygems.org"

gem "jekyll", ">= 4.3.0"
gem "kramdown-parser-gfm"
gem "webrick"         # optional, for local `jekyll serve` on Ruby 3

group :jekyll_plugins do
  gem "jekyll-email-protect"
  gem "jekyll-scholar"
  gem "jekyll-paginate"
  gem "jekyll-seo-tag"
  gem "jekyll-sass-converter", ">= 3.0.0"
end
